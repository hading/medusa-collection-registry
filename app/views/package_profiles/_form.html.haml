= simple_form_for @package_profile, html: {class: 'form-horizontal'}, wrapper: :horizontal_form do |f|
  .row
    = render partial: 'shared/form_sidebar', locals: {f: f, object: @package_profile, cancel_return: (package_profiles_path if @package_profile.new_record?)}
    .col-sm-9
      = f.input :name
      = f.input :url
      = f.input :notes