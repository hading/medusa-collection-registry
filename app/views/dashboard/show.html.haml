.tabbable
  = render :partial => 'shared/tab_header', :locals => {:id => 'tab-headers', :tabs => dashboard_tab_list}
  .tab-content
    #storage-overview.tab-pane.active
      = render 'storage_overview'
    #running-processes.tab-pane
      <p> you're in running process tab right now </p>
    #file-statistics.tab-pane
      = render 'file_stats_table'
    #red-flags.tab-pane
      = render 'red_flags'
:javascript
    $(document).ready(initializeChart($.parseJSON('#{@storage.to_json}')));
