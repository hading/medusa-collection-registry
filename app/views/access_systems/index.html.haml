- model_class = AccessSystem
.page-header
  %h1= t '.title', :default => model_class.model_name.human.pluralize

%table.table.table-striped#access_systems
  %thead
    %th Name
    %th
  %tbody
    - @access_systems.each do |access_system|
      %tr
        %td #{link_to access_system.name, for_access_system_collections_path(:access_system_id => access_system.id)}
        %td
          = link_to 'Edit', edit_access_system_path(access_system), :class => 'btn btn-mini'
          = link_to 'Delete', access_system, :method => :delete, :data => {:confirm => generic_confirm_message}, :class => 'btn btn-mini btn-danger'

%br

= link_to 'New Access System', new_access_system_path, :class => 'btn btn-primary'

