%h2 Access Systems
%p= link_to('New Access System', new_access_system_path)
%table#access_systems
  %tr Name
  %tr
  - @access_systems.each do |access_system|
    %tr
      %td #{link_to access_system.name, access_system}
      %td
        = link_to 'Edit', edit_access_system_path(access_system)
        |
        = link_to 'Delete', access_system, :method => :delete, :data => {:confirm => 'This is irreversible - are you sure?'}
