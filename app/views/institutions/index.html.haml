= render layout: 'layouts/page_header' do
  Institutions

%table.table.table-striped#institutions
  %thead
    %tr
      %th Name
      %th
  %tbody
    - @institutions.each do |institution|
      %tr
        %td= link_to(institution.name, institution)
        %td= small_edit_button(institution)

%br

= link_to 'New Institution', new_institution_path, class: 'btn btn-primary'
