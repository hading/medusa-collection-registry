= render 'layouts/standard_table', id: :file_format_tests, headers: ['File', 'Testers', 'Test Date', 'File Type', 'Status', 'Reasons', 'Comments'] do
  - @file_format_tests.find_each(batch_size: 10) do |file_format_test|
    %tr
      %td= link_to file_format_test.cfs_file_name, file_format_test.cfs_file
      %td= file_format_test.tester_email
      %td= file_format_test.date
      %td= file_format_test.content_type_name
      %td= file_format_test.pass_label
      %td= file_format_test.reasons_string
      %td= file_format_test.notes