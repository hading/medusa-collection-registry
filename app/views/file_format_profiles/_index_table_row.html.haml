%tr
  %td= link_to(file_format_profile.name, file_format_profile)
  %td= file_format_profile.status
  %td
    #{file_format_profile.software} (#{file_format_profile.software_version})
  %td
    #{file_format_profile.os_environment} (#{file_format_profile.os_version})
  %td= file_format_profile.content_types.collect {|content_type| content_type.name}.join(', ')
  %td= file_format_profile.file_extensions.collect {|file_extension| file_extension.extension}.join(', ')
  %td
    = small_view_button(file_format_profile)
    = small_clone_button(clone_file_format_profile_path(file_format_profile))
    = small_edit_button(file_format_profile)
